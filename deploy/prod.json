{
  "apps" : [{
    "name"      : "piaxi",
    "script"    : "./src/index.js",
    "instances" : "2",
    "exec_mode" : "cluster",
    "cwd": "/www/piaxi-se",
    "env": {
      "NODE_ENV": "production"
    },
    "error_file"        : "/var/log/piaxi-se/error.log",
    "out_file"          : "/var/log/piaxi-se/output.log",
    "merge_logs"        : true,
    "log_date_format"   : "YYYY-MM-DD HH:mm:ss.SSS Z",
    "watch"             : false,
    "max_memory_restart": "1G"
  }]
}
